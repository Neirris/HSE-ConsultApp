{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/auth(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/profile(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/users(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/chats(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/events(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/notifications(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
